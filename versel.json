{
  "version": 2,
  "builds": [
    {
      "src": "pages/**/*",
      "use": "@vercel/static"
    },
    {
      "src": "styles/**/*",
      "use": "@vercel/static"
    },
    {
      "src": "scripts/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/Matches/(.*)",
      "dest": "/pages/Matches/$1"
    },
    {
      "src": "/(.*\\.css)",
      "dest": "/styles/$1"
    },
    {
      "src": "/(.*\\.js)",
      "dest": "/scripts/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/pages/$1"
    }
  ]
}
